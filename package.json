{
  "name": "mdsv",
  "version": "0.0.0-dev",
  "description": "Bridging svelte and markdown.",
  "license": "MIT",
  "author": "Emmanuel Beaudry Marchand <emmbema@gmail.com>",
  "type": "module",
  "scripts": {
    "build": "pnpm --recursive --parallel --raw-output build",
    "dev": "pnpm --recursive --parallel --raw-output dev",
    "format": "pnpm lint:fix && pnpm prettier:fix",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "prettier": "prettier --check .",
    "prettier:fix": "prettier --write .",
    "test": "pnpm --recursive --parallel --raw-output test"
  },
  "devDependencies": {
    "@eslint/compat": "^1.2.8",
    "@eslint/js": "^9.25.1",
    "@standard-schema/spec": "^1.0.0",
    "@types/node": "^22.15.3",
    "eslint": "^9.25.1",
    "eslint-config-prettier": "^10.1.2",
    "eslint-plugin-import": "^2.31.0",
    "globals": "^16.0.0",
    "prettier": "^3.5.3",
    "prettier-plugin-jsdoc": "^1.3.2",
    "prettier-plugin-packagejson": "^2.5.10",
    "typescript": "^5.8.3"
  },
  "packageManager": "pnpm@10.7.0+sha512.6b865ad4b62a1d9842b61d674a393903b871d9244954f652b8842c2b553c72176b278f64c463e52d40fff8aba385c235c8c9ecf5cc7de4fd78b8bb6d49633ab6"
}
